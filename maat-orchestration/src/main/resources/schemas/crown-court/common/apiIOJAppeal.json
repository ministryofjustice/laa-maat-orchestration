{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "IOJAppeal.json",
  "type": "object",
  "description": "Crown Court IOJ appeal",
  "properties": {
    "decisionResult": {
      "type": "string",
      "title": "Rep Order decision result"
    },
    "iojResult": {
      "type": "string",
      "description": "Ioj Result"
    },
    "appealTypeCode": {
      "type": "string",
      "description": "Appeal type code"
    },
    "appealTypeDate": {
      "type": "string",
      "description": "Appeal type date",
      "format": "date-time"
    }
  },
  "additionalProperties": false,
  "required": ["decisionResult", "iojResult"]
}
